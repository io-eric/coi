// Test: iterating over array with for loop
component ArrayLoopTest {
    int[] values = [5, 10, 15, 20];
    mut int sum = 0;

    init {
        for i in 0:values.size() {
            sum = sum + values[i];
        }
    }

    view {
        <div>Sum: {sum}</div>
    }
}

app {
    root = ArrayLoopTest;
}
