// Test: using destructured tuple values
component TupleUse {
    mut int total = 0;
    
    def compute(int x): (int doubled, int squared) {
        return (x * 2, x * x);
    }
    
    def test(): void {
        (int d, int s) = compute(5);
        total = d + s;  // 10 + 25 = 35
    }

    view {
        <div>{total}</div>
    }
}

app {
    root = TupleUse;
}
