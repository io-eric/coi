// Test: binary operations and expressions
component BinaryOpsTest {
    int a = 10;
    int b = 3;
    mut int sum = 0;
    mut int diff = 0;
    mut int prod = 0;
    mut int quot = 0;
    mut int mod = 0;

    init {
        sum = a + b;
        diff = a - b;
        prod = a * b;
        quot = a / b;
        mod = a % b;
    }

    view {
        <div>
            <div>Sum: {sum}</div>
            <div>Diff: {diff}</div>
            <div>Product: {prod}</div>
            <div>Quotient: {quot}</div>
            <div>Modulo: {mod}</div>
        </div>
    }
}

app {
    root = BinaryOpsTest;
}
