// Test: match arm must end with semicolon (including last arm)
component MatchMissingSemicolonTest {
    enum Role {
        Admin,
        User
    }

    def label(Role role) : string {
        return match (role) {
            Role::Admin => "Admin";
            Role::User => "User"
        };
    }

    view {
        <div>{label(Role::Admin)}</div>
    }
}

app {
    root = MatchMissingSemicolonTest;
}
