// Test: compound index assignment (arr[i] += value)
component CompoundIndexAssignPass {
    mut int[] nums = [1, 2, 3, 4, 5];

    init {
        nums[0] += 10;   // 1 + 10 = 11
        nums[1] -= 1;    // 2 - 1 = 1
        nums[2] *= 2;    // 3 * 2 = 6
        nums[3] /= 2;    // 4 / 2 = 2
        nums[4] %= 3;    // 5 % 3 = 2
    }

    view {
        <div>Done</div>
    }
}

app {
    root = CompoundIndexAssignPass;
}
